def check_vowel_or_consonant(letter):
    vowels = ['a', 'e', 'i', 'o', 'u']

    if letter.lower() in vowels:
        return True
    else:
        return False

# Get input from the user
input_letter = input("Input a letter of the alphabet: ")

# Check if the input letter is a vowel or consonant
if check_vowel_or_consonant(input_letter):
    print(input_letter, "is a vowel.")
else:
    print(input_letter, "is a consonant.")
